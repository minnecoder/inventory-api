    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.7.2",
